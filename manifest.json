{

  "manifest_version": 2,
  "name": "Zoom Web Client Redirector",
  "version": "1.2.0",

  "description": "Redirect Zoom meetings to the web client version.",

  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png",
    "256": "icons/256x256.png"
  },

  "permissions": [
	   "tabs",
     "*://*.zoom.us/*"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.zoom.us/*"],
      "js": ["zoom-web-client-redirector.js"]
    }
  ]

}
